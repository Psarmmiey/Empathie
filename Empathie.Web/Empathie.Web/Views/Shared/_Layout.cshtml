                                            <li>@Html.ActionLink("Our Causes", "Causes")</li>
                                            <li>@Html.ActionLink("Single Cause", "#")</li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="#">Events</a>
                                        <ul>
                                            <li>@Html.ActionLink("Our Event", "Events")</li>
                                            <li>@Html.ActionLink("Event Details", "#")</li>

                                        </ul>
                                    </li>
                                    <li class="dropdown">
                                        <a href="#">Pages</a>
                                        <ul>
                                            <li>@Html.ActionLink("Donate Now", "Donate")</li>
                                            <li>@Html.ActionLink("Volunteer", "Volunteer")</li>
                                            <li>@Html.ActionLink("FAQS", "FAQS")</li>
                                            <li>@Html.ActionLink("404", "ErrorPage")</li>

                                        </ul>
                                    </li>
                                    <li>@Html.ActionLink("Gallery", "Gallery")</li>
                                    <li class="dropdown">
                                        <a href="#">Shop</a>
                                        <ul>
                                            <li><a href="shop.html">Our Shop</a></li>
                                            <li><a href="shop-single.html">Shop Single</a></li>
                                            <li><a href="shopping-cart.html">Shopping Cart</a></li>
                                        </ul>
                                    </li>
                                    <li class="dropdown">
