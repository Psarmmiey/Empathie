                                            <div class="post">
                                                <figure class="post-thumb"><img src="@Url.Content("~/Content/mytemplate/")images/resource/post-thumb-2.jpg" alt=""><a href="#" class="overlay-link"><span class="fa fa-link"></span></a></figure>
